--uncover tiles

function add_tile(x,y)
		local sprite = mget(x+offset,y)
		if not(fget(mget(x,y),1)) then
				add(tiles, {x=x, y=y, sprite=sprite})
		end
end

function flood(x, y)
		if fget(mget(x,y),0) and not fget(mget(x-offset,y+offset),5) then
				hit_bomb(x, y)
		else
				if fget(mget(x,y),2) then
						if not(fget(mget(x,y+offset),3)) then
								mset(x,y+offset, 016)
								 flood(x + 1, y)     
					    flood(x - 1, y)     
					    flood(x, y + 1)    
					    flood(x, y - 1)    
					    flood(x + 1, y + 1) 
					    flood(x + 1, y - 1) 
					    flood(x - 1, y + 1) 
					    flood(x - 1, y - 1) 
						end
		  else
		  		if mget(x,y+offset) == 17 then
		    		mset(x,y+offset, 016)
		    end
		  end
		end	
end



function render_tiles()
		for i=1, #tiles do
				local tp = tiles[i]
				mset(tp.x,tp.y,tp.sprite)
		end
end

function flag(x,y)
		sfx(9)
		if mget(x,y) == 018 then
				num_flags += 1
				mset(x,y,000)
		else
				num_flags -= 1
				mset(x,y,018)
		end
end

function uncover_surround(x,y)
		local bomb_count = 0
		local flag_count = 0
		local continue = true
		
		if fget(mget(x+offset,y+offset),3) then
				for i=-1,1 do
						for j=-1,1 do
								if	not(fget(mget(x+offset+i,y+j),0) == fget(mget(x+i,y+j+offset),5)) then
										continue = false
								end
						end
				end
				
				if continue == true then
						for i=-1,1 do
								for j=-1,1 do
										flood(x+i+offset,y+j)
								end
						end
				end
		end
end


