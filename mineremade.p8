--checks

function hit_bomb(x, y)
		game_over = true
		lost = true
		
		for i=map_x, map_size+map_x do
				for j=map_y, map_size+map_y do
						if fget(mget(i+offset,j),0) then
								mset(i,j+offset,032)
						end
				end
		end
		mset(x-offset,y+offset, 019)
end

function check_win()
		tiles_left = 0
		for i=map_x, map_size+map_x do
				for j=map_y, map_size+map_y do
						if mget(i ,j) == 001 or mget(i ,j) == 021 then
								tiles_left += 1
						end
				end
		end
		if tiles_left == num_boms then
			 
				game_over = true
				lost = false
				
				for i=map_x, map_size+map_x do
						for j=map_y, map_size+map_y do
								if fget(mget(i+offset,j),0) then
										mset(i,j+offset,033)
								end
						end
				end
				
		end
end


