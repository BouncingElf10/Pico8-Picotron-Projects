function frame()
		mset(map_x-1,map_y-1,61)
		for i=0, map_size do
				mset(map_x+i,map_y-1,58)
		end
		mset(map_x+map_size+1,map_y-1,62)
		for i=0, map_size do
				mset(map_x+map_size+1,map_y+i,57)	
		end
		mset(map_x+map_size+1,map_y+map_size+1,63)
		for i=0, map_size do
				mset(map_x+i,map_y+map_size+1,56)
		end
		mset(map_x-1,map_y+map_size+1,60)
		for i=0, map_size do
				mset(map_x-1,map_y+i,59)	
		end
end

