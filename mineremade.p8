__lua__
start_game = false
difficulty = 0
function _init()
		poke(0x5f2d, 1)
		if difficulty == 0 then
				init_small()
		else
				init_big()
		end
		if start_game == false then
				music(0)
		end
		debug=false
		if debug then
				init_debug()
		end
end

function _update()
		--cur_x = stat(32)-1/8
		--cur_y = stat(33)-1/8
		
		update_cur()
		if start_game == false then
				menu_button()
		end
	
		if shake == true and shake_timer > 0 then
		  local shakex=16-rnd(32)
			 local shakey=16-rnd(32)
			
			 shakex*=shakein
			 shakey*=shakein
			
			 camera(shakex,shakey)
			 if shakein == 1 then
			 		sfx(6)
			 		sfx(7)
			 end
			 shakein = shakein*0.85
			 if (shakein<0.05) shakein=0
		  shake_timer -= 1
		else
		  camera(0,0)
		end
end

function _draw()
		cls()
		--spr(12,stat(32)-1,stat(33)-1)
		if start_game == true then
				if temp_stop then
						music(-1)
				end
				draw_map(true)
				check_win()
				--render_tiles() --redundant
				map(0, 0, 0, 0, 16, 16)
				mset(cur_x,cur_y,002)
				map(0, 0, 0, 0, 16, 16)
				buttons()
				if not debug then
						title(4,3)
						frame()
				end
				if not game_over then
						if difficulty == 0 then
								print("flags ("..num_flags.."/"..num_boms..")", 37, map_size*8+16+map_y*8,7)
						else
								print("‚óÜ", 1, 24, 7)
								print(num_flags)
								print("/")
								print(num_boms)
						end
				end
		else
				if	time() % 1 == 0 then
						init_map()
				end
				for i=map_x, map_size+map_x do
						for j=map_y, map_size+map_y do
								cur_x = i
								cur_y = j
								flood(cur_x + offset, cur_y)
						end
				end
				draw_map(false)
				map(0, 0, 0, 0, 16, 16)
				title(4,3)
				if menu_page == 0 then
						rectfill(0,map_size*8+40, 127,127, 0)
						rectfill(29, 97,88,116, 0)
						rectfill(28, 98,89,115, 0)
						print("start game", 40, 100, 7)
						print("difficulty", 40, 109, 6)
						menu()
				else
						rectfill(0,map_size*8+40, 127,127, 0)
						rectfill(28, 97,89,124, 0)
						if difficulty == 0 then
								rectfill(50, 99, 66, 105, 7)
						else
								rectfill(50, 108, 66, 114, 7)
						end
						print("easy", 51, 100, 11)
						print("hard", 51, 109, 8)
						print("go back", 45, 118, 7)
						menu()
				
				end
		end
		
end
