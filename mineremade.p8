--func
function init_map()
		for i=0, 99 do
				for j=0, 99 do
						mset(i,j,000)
				end
		end
		
		for i=map_x, map_size+map_x do
				for j=map_y, map_size+map_y do
						mset(i+offset,j+offset,017)
				end
		end
		
		for i=map_x, map_size+map_x do
				for j=map_y, map_size+map_y do
						mset(i+offset,j,003)
				end
		end
		
		init_bomb_pos()
		
		for i=map_x, map_size+map_x do
				for j=map_y, map_size+map_y do
						set_number_tiles(i+offset,j)
				end
		end
end

function draw_map(overlay)
		if overlay then
				for i=map_x, map_size+map_x do
						for j=map_y, map_size+map_y do
								mset(i,j,001)
						end
				end
		end
		
		for i=map_x, map_size+map_x do
				for j=map_y, map_size+map_y do
						if fget(mget(i+offset,j+offset),3) then
								mset(i,j,mget(i+offset,j))
						end
				end
		end
		
		for i=map_x, map_size+map_x do
				for j=map_y, map_size+map_y do
						if mget(i,j+offset) == 018 then
								mset(i,j,021)
								mset(i+offset,j+offset,017)
						end
						if mget(i,j+offset) == 019 then
								mset(i,j,020)
						end
						if mget(i,j+offset) == 032 then
								mset(i,j,004)
						end
						if mget(i,j+offset) == 033 then
								mset(i,j,022)
						end
				end
		end
		
		
		if game_over == true then
				if lost == true then
						print("game over!", 0, map_size*8+25+map_y*8,7)
						if difficulty == 1 then
								if start_game == true then
										print("‚ùé", 1, 32, 7)
										spr(014, 1,38)
										print("üÖæÔ∏è", 1, 50, 7)
										spr(014, 1,56)
										rectfill(1,57,8,64,0)
										spr(015, 1,58)
								end
						end
						
						if start_game == true then
								shake = true
						end
				else
						print("you win!!", 0, map_size*8+25+map_y*8,7)
						if won == false then
								sfx(8)
								music(3)
								won = true
						end
				
				end
				cur_x = 99
				cur_y = 99
				print("press üÖæÔ∏è to restart", 50, map_size*8+21+map_y*8,7)
				print("press ‚ùé to return", 50, map_size*8+21+map_y*8+8,7)
		end
end

function buttons()
		if btnp(‚ùé) then
				if game_over == true then
						music(-1)
						music(0)
						
						_init()
						start_game = false
				end
			--	add_tile(cur_x,cur_y) --flood takes care of this /w tables
				uncover_surround(cur_x, cur_y)
				if not(fget(mget(cur_x,cur_y+offset),5)) then
						sfx(10)
						flood(cur_x + offset, cur_y)
				end
				
		end
		if temp_stop == false then
				if btnp(üÖæÔ∏è) then
						if fget(mget(cur_x+offset,cur_y+offset),4) then
								flag(cur_x, cur_y + offset)
						end
						if game_over == true then
						  music(-1)
						  sfx(4)
						  _init()
						end
				end
		end
		temp_stop = false
end

