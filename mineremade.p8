--cur & init

function update_cur()
				if btnp(0) then
						cur_x -= 1
				elseif btnp(1) then
						
						cur_x += 1
				elseif btnp(2) then
						
						cur_y -= 1
				elseif btnp(3) then
						
						cur_y += 1
				end
		
		
		if cur_y > map_size+map_y then
				cur_y = map_y
				
		end
		if cur_y < map_y then
				cur_y = map_size+map_y
		end
		if cur_x > map_size+map_x then
				cur_x = map_x
		end
		if cur_x < map_x then
				cur_x = map_size+map_x
		end
end

function init_bomb_pos()
		local temp_bomb = num_boms
		
		while temp_bomb > 0 do
				local x = rnd(map_size+1)+offset+map_x
				local y = rnd(map_size+1)+map_y
				
				
				if not(fget(mget(x, y), 0)) then
  				mset(x,y, 004)
  				temp_bomb -= 1
  		end
		end
end

function set_number_tiles(x, y)
    num_mines = 0
    
    for i = -1, 1 do
      for j = -1, 1 do
    				if fget(mget(x+i, y+j), 0) then
    						num_mines += 1
    				end
      end
    end 
    
    if num_mines != 0 then
    		if not(fget(mget(x, y), 0)) then
		    		sprite = 004 + num_mines
		    
		    		mset(x, y, sprite)
		    end
   	end
end

