--[[pod_format="raw",created="2025-02-06 12:36:16",modified="2025-03-03 09:20:59",revision=781]]
-- Draws a single line of the mandlebrot fractal from 0, to 478
function drawMandlebrotLine(j, scale_factor)
	for i=0, 478 / scale_factor do
		x,y = convertPosition(i *scale_factor,j*scale_factor, zoom, tx, ty) -- Screen space to Mandelbrot range
		if not mandlebrot then escape = checkBurningShip(x, y, 200) end -- Checks for the mandlebrot
		if mandlebrot then escape = checkMandlebrot(x, y, 200) end -- Checks for the mandlebrot
		
		-- Chooses color
		if not default_color then brot_color =  26 - escape / (200/26) end
		if default_color then brot_color =  escape / (200/57) end
		if escape >= 200 then brot_color = 54 end
		rectfill(i * scale_factor, j * scale_factor, (i + 1) * scale_factor - 1, (j + 1) * scale_factor - 1, brot_color) -- Render a pixel of the mandlebrot
	end
end

-- Screen space to Mandelbrot range
function convertPosition(mx, my, zoom, tx, ty)
	zoom = zoom or 1
	tx = tx or 0
	ty = ty or 0
	
	-- Map x from screen [0,480] to Mandelbrot range [-2.5, 1]
	local normalized_x = (((mx / 480) * 3.5) - 2.5) / zoom + tx
	
	-- Map y from screen [0,270] to Mandelbrot range [-1, 1]
	local normalized_y = (((my / 270) * 2) - 1) / zoom + ty
	
	return normalized_x, normalized_y
end



function checkMandlebrot(real_c, imag_c, max_iterations)
    -- Early escape for main cardioid and period-2 bulb
    local q = (real_c - 0.25) * (real_c - 0.25) + imag_c * imag_c
    if q * (q + (real_c - 0.25)) <= 0.25 * imag_c * imag_c then
        return max_iterations
    end
    if (real_c + 1) * (real_c + 1) + imag_c * imag_c <= 0.0625 then
        return max_iterations
    end

    local real = 0.0
    local imag = 0.0
    local real_sq, imag_sq
    
    for i=0, max_iterations do
        real_sq = real * real
        imag_sq = imag * imag
        if real_sq + imag_sq > 4.0 then
            return i
        end
        imag = 2 * real * imag + imag_c
        real = real_sq - imag_sq + real_c
    end
    return max_iterations
end

function checkBurningShip(real_c, imag_c, max_iterations)

    local real = 0.0
    local imag = 0.0
    local real_sq, imag_sq
    
    for i = 0, max_iterations do
        real_sq = real * real
        imag_sq = imag * imag
        if real_sq + imag_sq > 4.0 then
            return i
        end
        
        imag = math.abs(2 * real * imag) + imag_c
        real = real_sq - imag_sq + real_c
    end
    
    return max_iterations
end

