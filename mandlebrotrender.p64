--[[pod_format="raw",created="2025-02-06 12:22:21",modified="2025-03-03 11:03:55",revision=1240]]
include"functions.lua"

function _init()
	------------SETTINGS------------
	default_color = true -- Changes the color palette
	mandlebrot = true -- If false it displays the burning ship factal

	------------________------------
	poke4(0x5000, get(fetch"pal/0.pal"))

	--Init vars
	height = 270
	width = 480
	ratio = 480 / 270
	scale_factor = 1
	itterations = 150
	zoom = 1
	tx = 0
	ty = 0
	
	time_passes = 0
	time_step = 2
	change_time = 0
	render_complete = false
end

function _update()

	
	
	-- Translaction and scale variables
	zoom_factor = zoom * 0.05 
	translation_factor = 0.1 / zoom
	
	if btn(4) then zoom -= zoom_factor end
	if btn(5) then zoom += zoom_factor end
	if btn(0) then tx -= translation_factor end
	if btn(1) then tx += translation_factor end
	if btn(2) then ty -= translation_factor end
	if btn(3) then ty += translation_factor end
	
	-- If it gets to the 3rd pass it marks the frame as "finished"
	if time_passes > 2 then
		render_complete = true
		return
	end
	
	--"change_time" is what y strip is being rendered
	change_time += time_step
	if change_time > height then -- If it finished 1 pass (change_time goes past the screen boundaries)
		change_time -= height + 1 -- Add offset to 2nd pass
		time_passes += 1
	end
	-- Once weve done 2 passes with a step of 2 we start the 3rd final pass
	if time_passes > 1 then
		time_step = 1
		time_step = stat(7) / 60.0 -- Makes sure no lines are skipped
	end
end


function _draw()

	if btn() != 0 then -- If movement is detetched render a very low-res, real-time version of the fractal
		itterations = 20 -- Saves performance (less itterations == less cpu time)
		for i=0, 270 / 15 do
			drawMandlebrotLine(i, 15)
		end
		time_step = 2
		change_time = 0
		time_passes = 0
		render_complete = false
		return
	end
	
	if btn() == 0 then -- Change to full itteration count if being rendered
		itterations = 150
	end

	drawMandlebrotLine(change_time, 1)
	
	-- Render debug 
	rectfill(479, 0,	479, 269, render_complete and 24 or 0)  
	pset(479, change_time, 24)
	
--	rectfill(0,100,50,0,20)
--	print(change_time,0,0,0)
--	print(time_passes)
--	print(time_step)
--	print(itterations)
end

